<!DOCTYPE html>
<html>
<head>
  <title>Atgdb</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="wrapper">
		<div id="header">
			<h1>ATGDB</h1>
			<div id="navigation">
				<ul>
					<li><%= link_to("Players", :players) %></li>
					<li><%= link_to("Managers", :managers) %></li>
					<li><%= link_to("Teams", :teams) %></li>
				</ul>
			</div>
		</div>
		<div id="content">
			<%= yield %>
		</div>
	</div>
</body>
</html>